@model IEnumerable<WebMap.Models.UserModel>
@{
    ViewBag.Title = "Index";
}

<table class="table">
    <thead>
        <tr>

            <th>Id</th>
            <th>State</th>
            <th>Date Apply</th>
            <th>Speciality</th>

            <th>Id Resource</th>
            <th class="text-nowrap">Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td><a href="javascript:void(0)">@Html.DisplayFor(modelItem => item.firstname)</a></td>
                <td>@Html.DisplayFor(modelItem => item.lastname)</td>
                <td><span class="text-muted"><i class="fa fa-clock-o"></i>@Html.DisplayFor(modelItem => item.Email)</span> </td>
                <td><span class="label label-warning">@Html.DisplayFor(modelItem => item.address)</span></td>




                <td>@Html.DisplayFor(modelItem => item.phoneNumber)</td>
                <td class="text-nowrap">
                    <a href="#" data-toggle="tooltip" data-original-title="Edit"> <i class="fa fa-pencil text-inverse m-r-10"></i> </a>
                    <a href="#" data-toggle="tooltip" data-original-title="Close"> <i class="fa fa-close text-danger"></i> </a>
                </td>
                <td>
                    @Html.ActionLink("Delete", "Delete", new { id = item.id })
                    @Html.ActionLink("Affecter","Affecter",new { id = item.id})
                    
                </td>
                

            </tr>
        }

    </tbody>
</table>