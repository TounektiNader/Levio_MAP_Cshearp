
@model Domain.Entities.Claim


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/TempleAdmin.cshtml";
}
<div class="row page-titles">
    <div class="col-md-5 align-self-center">
        <h3 class="text-themecolor">@Html.ActionLink("Back to List", "Index")</h3>

    </div>
    <div class="col-md-7 align-self-center">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="javascript:void(0)">Claim/list of complaints/Details</a></li>
          
        </ol>
    </div>
    <div>
        <button class="right-side-toggle waves-effect waves-light btn-inverse btn btn-circle btn-sm pull-right m-l-10">
        <i class="ti-settings text-white"></i></button>
    </div>
</div>
<!-- ============================================================== -->
<!-- End Bread crumb and right sidebar toggle -->
<!-- ============================================================== -->
<!-- ============================================================== -->
<!-- Container fluid  -->
<!-- ============================================================== -->

<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="row">
                
                <div class="col-xlg-10 col-lg-12 col-md-8 bg-light-part b-l">
                    
                    <div class="card-body p-t-0">
                        <div class="card b-all shadow-none">
                            <div class="card-body">
                                <h3 class="card-title m-b-0">@Html.DisplayFor(model => model.type)</h3>

                            </div>
                            <div>
                                <hr class="m-t-0">
                            </div>
                            <div class="card-body">
                                <div class="d-flex m-b-40">
                                    <div>
                                        <a href="javascript:void(0)"><img src="~/ContentA/assets/images/users/1.jpg" alt="user" width="40" class="img-circle" /></a>
                                    </div>
                                    <div class="p-l-10">
                                        <h4 class="m-b-0">@Html.DisplayFor(model => model.user.firstname) @Html.DisplayFor(model => model.user.lastname)</h4>
                                        <small class="text-muted">@Html.DisplayFor(model => model.user.email)</small>
                                    </div>
                                </div>
                                <p><b>@Html.DisplayFor(model => model.topic)</b></p>
                               
                            </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>


<!-- End Page Content -->











